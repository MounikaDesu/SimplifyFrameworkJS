{
    "Profile": "{{deploymentProfile}}",
    "Region": "{{deploymentRegion}}",
    "Bucket": {
        "Name": "{{apiNamePosix}}",
        "Key": "functions/{{serviceNamePosix}}/${DATE_TODAY}/builds"
    },
    "OutputFile": "function-meta.json",
    "Function": {
        "FunctionName": "{{serviceName}}",
        "Handler": "index.handler",
        "MemorySize": 256,
        "Publish": true,
        "Role": "${Function{{serviceNamePascal}}RoleArn}",
        "Runtime": "nodejs12.x",
        "Tags": {
            "GroupName": "{{serviceTag}}"
        },
        "Timeout": 15,
        "TracingConfig": {
            "Mode": "Active"
        },
        "Environment": {
            "Variables": {
                "DEPLOYMENT": "latest"
            }
        }
    }
}