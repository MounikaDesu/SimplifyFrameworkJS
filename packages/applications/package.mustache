{
    "name": "{{projectNamePosix}}",
    "version": "{{projectVersion}}",
    "description": "{{{apiDescription}}}",
    "main": "app.js",
    "scripts": {
        "build": "nyc --reporter=html mocha --require @babel/register --reporter mocha-simple-html-reporter --reporter-options output=index.html --timeout 60000 \"./tests/**/*.spec.js*\"",
        "start": "node app.js",
        "deploy": "docker-compose up -d",
        "test": "nyc --reporter=text mocha --require @babel/register --timeout 60000 \"./tests/**/*.spec.js*\"",
        "test-units": "nyc --reporter=text mocha --require @babel/register --timeout 60000 \"./tests/**/units/*.spec.js*\"",
        "test-apis": "nyc --reporter=text mocha --require @babel/register --timeout 60000 \"./tests/**/apis/*.spec.js*\"",
        {{#services}}
        "test-{{serviceNamePosix}}": "mocha --require @babel/register --timeout 60000 \"./tests/{{serviceName}}/apis/*.spec.js*\"",
        {{/services}}
        "coverage": "nyc report --reporter=text-lcov | coveralls"
    },
    "keywords": [
        "swagger"
    ],
    "nyc": {
        "exclude": [
            "**/*.spec.js",
            "**/*.env.js"
        ]
    },
    "license": "MIT",
    "private": true,
    "dependencies": {
        "aws-sdk": "^2.635.0",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "debug": "^4.1.1",
        "brakes": "^2.8.0",
        "swagger-express-middleware": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.6",
        "@babel/node": "^7.7.4",
        "@babel/preset-env": "^7.9.6",
        "@babel/register": "^7.9.0",
        "amazon-cognito-identity-js": "^3.1.3",
        "crypto-js": "^3.1.9-1",
        "node-fetch": "^2.6.0",
        "case": "^1.5.4",
        "simplify-sdk": "^0.1.7",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "mocha-simple-html-reporter": "^1.1.0",
        "aws-sdk": "^2.592.0",
        "adm-zip": "^0.4.14",
        "mocha": "^7.0.1",
        "nyc": "^15.0.0",
        "rewire": "^4.0.1",
        "coveralls": "^3.0.9"
    }
}
