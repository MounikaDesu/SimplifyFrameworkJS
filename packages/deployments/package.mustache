{
    "name": "{{projectNamePosix}}",
    "version": "{{projectVersion}}",
    "description": "{{{projectDescription}}}",
    "scripts": {
        "start": "npm install && npm run gencode",
        "gencode": "simplify-codegen generate -i openapi.yaml -o . --merge",
        "stack-deploy": "node deployment.js --input {{deploymentNamePosix}}.json",
        "stack-destroy": "node deployment.js --input {{deploymentNamePosix}}.json --deletion",
        "push-update": "bash code-push.sh .env update",
        "push-code": "bash code-push.sh .env push"
    },
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "simplify-sdk": "^0.1.7"
    }
}
