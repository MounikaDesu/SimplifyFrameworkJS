{
    "name": "{{projectNamePosix}}",
    "version": "{{projectVersion}}",
    "description": "{{{projectDescription}}}",
    "scripts": {
        "start": "npm install && npm run gencode",
        "gencode": "simplify-codegen generate -i openapi.yaml -o . --merge",
        "deploy": "node deployment.js --input {{deploymentNamePosix}}.json",
        "destroy": "node deployment.js --input {{deploymentNamePosix}}.json --deletion",
        "stable": "bash code-push.sh stable",
        "latest": "bash code-push.sh latest"
    },
    "license": "MIT",
    "private": true,
    "devDependencies": {
        "simplify-sdk": "^0.1.6"
    }
}
