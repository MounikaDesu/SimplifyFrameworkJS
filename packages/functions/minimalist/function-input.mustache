{
    "Profile": "{{deploymentProfileOrigin}}",
    "Region": "{{deploymentRegionOrigin}}",
    "Bucket": {
        "Name": "{{projectNamePosix}}",
        "Key": "functions/{{serviceNamePosix}}/${DATE_TODAY}/builds"
    },
    "OutputFile": "function-meta.json",
    "Layer": {
        "CompatibleRuntimes": [
            "{{serviceRuntime}}"
        ],
        "LayerName": "{{serviceNamePosix}}"
    },
    "Function": {
        "FunctionName": "{{serviceName}}",
        "Handler": "index.handler",
        "MemorySize": 128,
        "Publish": true,
        "Role": "${Function{{serviceNamePascal}}RoleArn}",
        "Runtime": "{{serviceRuntime}}",
        "Tags": {
            "GroupName": "{{serviceTag}}"
        },
        "Timeout": 15,
        "TracingConfig": {
            "Mode": "PassThrough"
        },
        "Environment": {
            "Variables": {
                "DEPLOYMENT_STAGE": "${DEPLOYMENT_STAGE}",
                "ENFORCEMENT_PACKAGE": "${ENFORCEMENT_PACKAGE}"
            }
        }
    }
}