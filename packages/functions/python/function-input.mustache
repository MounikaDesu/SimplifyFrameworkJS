{
    "Profile": "{{deploymentProfileOrigin}}",
    "Region": "{{deploymentRegionOrigin}}",
    "Bucket": {
        "Name": "{{serviceNamePosix}}-{{projectId}}",
        "Key": "functions/{{serviceNamePosix}}/${DATE_TODAY}/dist"
    },
    "OutputFile": "function-meta.json",
    "Function": {
        "FunctionName": "{{serviceName}}",
        "Handler": "index.handler",
        "MemorySize": 256,
        "Publish": true,
        "Role": "${Function{{serviceNamePascal}}RoleArn}",
        "Runtime": "{{serviceRuntimeOrigin}}",
        "Tags": {
            "GroupName": "{{serviceTag}}"
        },
        "Timeout": 15,
        "TracingConfig": {
            "Mode": "Active"
        },
        "Environment": {
            "Variables": {
                "ENVIRONMENT": "development"
            }
        }
    }
}